<application>
  <component name="CodeInsightSettings">
    <option name="REFORMAT_ON_PASTE" value="1" />
    <option name="INDENT_TO_CARET_ON_PASTE" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Fira Code Medium" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="USE_LIGATURES" value="true" />
    <option name="SECONDARY_FONT_FAMILY" value="JetBrains Mono" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\binf\b.*">
      <option name="FOREGROUND" value="19d70d" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
  </component>
</application>